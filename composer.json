{
  "name": "romaten1/craft3-weightedsearch",
  "description": "A remake of the weighted search plugin for Craft 2",
  "type": "craft-plugin",
  "version": "1.0.7",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "weighted search",
    "craft3"
  ],
  "support": {
    "docs": "https://github.com/romaten1/craft3-weightedsearch/blob/master/README.md",
    "issues": "https://github.com/romaten1/craft3-weightedsearch/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Venveo",
      "homepage": "https://www.venveo.com"
    },
    {
      "name": "Ransom Roberson",
      "homepage": "https://www.ransom.pw"
    },
    {
      "name": "romaten1",
      "homepage": "https://github.com/romaten1"
    }
  ],
  "require": {
    "craftcms/cms": "^3.0.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "romaten1\\weightedsearch\\": "src/"
    }
  },
  "extra": {
    "name": "Weighted Search",
    "handle": "craft3-weightedsearch",
    "schemaVersion": "1.0.0",
    "hasCpSettings": false,
    "hasCpSection": false,
    "changelogUrl": "https://raw.githubusercontent.com/venveo/craft3-weightedsearch/master/CHANGELOG.md",
    "components": {
      "entriesService": "romaten1\\weightedsearch\\services\\EntriesService"
    },
    "class": "romaten1\\weightedsearch\\WeightedSearch"
  }
}
